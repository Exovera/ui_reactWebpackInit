{
	"name": "reactWebpackInit",
	"version": "1.0.0",
	"main": "index.js",
	"license": "MIT",
	"scripts": {
		"build": "webpack --config webpack.prod.js",
		"start": "webpack-dev-server --config webpack.dev.js --hot",
		"deploy-lite": "aws s3 rm s3://narratives-lite --recursive && aws s3 cp --cache-control max-age=31536000 --exclude index.html dist s3://narratives-lite --recursive && aws s3 cp dist/index.html s3://narratives-lite --cache-control no-cache",
		"test": "jest --coverage --runInBand",
		"testcurrent": "jest --watch --runInBand -t 'Navbar'",
		"watchtest": "jest --watch --runInBand",
		"prettierwhole": "prettier --print-width 100 --use-tabs --trailing-comma es5 --arrow-parens always --write 'src/**/*.jsx'"
	},
	"dependencies": {
		"@exovera/helpers": "^0.1.44",
		"@exovera/icons": "^0.1.37",
		"@exovera/narrative-tracker-variables": "^0.1.27",
		"@exovera/style-mixins": "^0.1.2",
		"babel-plugin-styled-components": "^1.10.0",
		"classnames": "^2.2.5",
		"cross-fetch": "^3.0.6",
		"css-loader": "^0.28.7",
		"es6-promise": "^4.1.1",
		"file-loader": "^0.11.2",
		"history": "^4.7.2",
		"moment": "^2.29.0",
		"path": "^0.12.7",
		"prop-types": "^15.5.10",
		"react": "^16.13.1",
		"react-addons-shallow-compare": "^15.6.0",
		"react-addons-test-utils": "^15.6.2",
		"react-custom-scrollbars": "^4.2.1",
		"react-dom": "^16.13.1",
		"react-hot-loader": "^4.13.0",
		"react-loadable": "^5.3.1",
		"react-measure": "^2.5.2",
		"react-onclickoutside": "^6.9.0",
		"react-redux": "^7.2.1",
		"react-router-dom": "^4.2.2",
		"react-router-redux": "next",
		"react-test-renderer": "^16.3.2",
		"recompose": "^0.30.0",
		"redux": "^4.0.5",
		"redux-thunk": "^2.2.0",
		"style-loader": "^0.18.2",
		"styled-components": "^5.2.0",
		"underscore": "^1.12.1",
		"url-loader": "^4.1.0"
	},
	"devDependencies": {
		"babel-core": "^6.26.0",
		"babel-eslint": "^8.2.3",
		"babel-jest": "^22.4.3",
		"babel-loader": "^7.1.4",
		"babel-plugin-syntax-dynamic-import": "^6.18.0",
		"babel-polyfill": "^6.26.0",
		"babel-preset-env": "^1.6.1",
		"babel-preset-react": "^6.24.1",
		"clean-webpack-plugin": "^0.1.19",
		"copy-webpack-plugin": "^4.5.1",
		"enzyme": "^3.3.0",
		"enzyme-adapter-react-16": "^1.1.1",
		"enzyme-to-json": "^3.3.3",
		"eslint": "^4.19.1",
		"eslint-config-airbnb": "^16.1.0",
		"eslint-config-prettier": "^2.9.0",
		"eslint-plugin-import": "^2.11.0",
		"eslint-plugin-jsx-a11y": "^6.0.3",
		"eslint-plugin-prettier": "^2.6.0",
		"eslint-plugin-react": "^7.7.0",
		"html-loader": "^0.5.5",
		"html-webpack-plugin": "^3.2.0",
		"jest": "^26.5.0",
		"less": "^2.7.2",
		"less-loader": "^4.0.2",
		"mini-css-extract-plugin": "^0.11.3",
		"nightmare": "^3.0.2",
		"nightmare-real-mouse": "^1.1.0",
		"postcss-loader": "^2.1.3",
		"prettier": "^2.1.2",
		"prettier-eslint": "^8.8.1",
		"webpack": "^4.29.0",
		"webpack-cli": "^3.2.1",
		"webpack-dev-server": "^3.11.0",
		"webpack-preset": "^0.2.0",
		"webpack-preset-babel": "^0.2.0",
		"webpack-preset-babel-stage-2": "^0.2.0",
		"webpack-visualizer-plugin": "^0.1.11"
	},
	"eslintConfig": {
		"parser": "babel-eslint",
		"extends": [
			"plugin:import/errors"
		],
		"env": {
			"mocha": true
		}
	},
	"jest": {
		"modulePaths": [
			"<rootDir>/src"
		],
		"moduleFileExtensions": [
			"js",
			"jsx"
		],
		"moduleDirectories": [
			"node_modules"
		],
		"collectCoverageFrom": [
			"client/*.js",
			"client/*.jsx"
		],
		"coverageThreshold": {
			"global": {
				"branches": 1,
				"functions": 1,
				"lines": 1,
				"statements": 1
			}
		},
		"setupTestFrameworkScriptFile": "<rootDir>/src/config/jestSetup.js"
	}
}
